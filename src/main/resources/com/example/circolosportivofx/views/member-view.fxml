<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="360.0" prefWidth="540.0" styleClass="root-pane" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.circolosportivofx.MemberController">
   <children>
      <Label fx:id="titleLabel" alignment="CENTER" layoutX="53.0" layoutY="53.0" prefHeight="53.0" prefWidth="434.0" styleClass="title" text="Benvenuto" />
      <ComboBox fx:id="comboSubscribe" layoutX="53.0" layoutY="145.0" prefHeight="37.0" prefWidth="247.0" styleClass="custom-combobox" />
      <ComboBox fx:id="comboUnsubscribe" layoutX="53.0" layoutY="232.0" prefHeight="37.0" prefWidth="247.0" styleClass="custom-combobox" />
      <Button fx:id="buttonSubscribe" layoutX="337.0" layoutY="145.0" mnemonicParsing="false" onAction="#subscribe" prefHeight="37.0" prefWidth="150.0" styleClass="custom-button" text="Subscribe" />
      <Button fx:id="buttonUnsubscribe" layoutX="337.0" layoutY="232.0" mnemonicParsing="false" onAction="#unsubscribe" prefHeight="37.0" prefWidth="150.0" styleClass="custom-button" text="Unsubscribe" />
      <Label fx:id="resultLabel" layoutX="58.0" layoutY="301.0" prefHeight="17.0" prefWidth="434.0" styleClass="result-label" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="318.0" mnemonicParsing="false" text="Back" onAction="#previousScene" />
      <Button fx:id="logoutButton" layoutX="474.0" layoutY="14.0" mnemonicParsing="false" text="Logout" onAction="#logout" />
   </children>
</AnchorPane>
